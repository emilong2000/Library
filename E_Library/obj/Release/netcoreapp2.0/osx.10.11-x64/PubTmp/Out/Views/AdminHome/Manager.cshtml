@{
    ViewData["Title"] = "Manage Books and Videos";
}
<h3>@ViewData["Title"]</h3>
<div class="container">
    <div class="mdl-grid">
        <div class="mdl-cell mdl-cel--6-col">
            <div class="demo-card-square mdl-card mdl-shadow--1dp">
                <div class="mdl-card__title mdl-card--expand">

                    <h2 class="mdl-card__text homeLabel"> <img src="~/images/managebook.jpg" alt="Book Manager" /></h2>
                </div>
                <div class="mdl-card__supporting-text">
                    View and manage library books.
                </div>
                <div class="mdl-card__actions mdl-card--border">
                    <a class="mdl-button mdl-button--colored" asp-controller="Book" asp-action="AssetList">
                        View Books
                    </a>
                </div>
            </div>
        </div>   
        <div class="mdl-cell mdl-cel--6-col">
            <div class="demo-card-square mdl-card mdl-shadow--1dp">
                <div class="mdl-card__title mdl-card--expand">

                    <h2 class="mdl-card__text homeLabel"><img src="~/images/managevideo.jpg" alt="Video Manager" /></h2>
                </div>
                <div class="mdl-card__supporting-text">
                    View and manage Emilong library videos.
                </div>
                <div class="mdl-card__actions mdl-card--border">
                    <a class="mdl-button mdl-button--colored" asp-controller="Video" asp-action="VideoList">
                        View Videos
                    </a>
                </div>
            </div>
        </div>
    </div>

</div>
